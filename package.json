  "description": "Angular2 Superadmin with Webpack",
  "scripts": {
    "start": "set NODE_ENV=development && webpack-dev-server --inline --progress --port 3000",
    "test": "set NODE_ENV=test && karma start",
    "build": "set NODE_ENV=production && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "clean": "rimraf dist",
    "static": "http-server dist -p 8090 -o",
    "postinstall": "typings install"
